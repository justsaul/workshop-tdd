{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src": [
        "./src"
      ],
      "src/*": [
        "./src/*"
      ]
    },
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
